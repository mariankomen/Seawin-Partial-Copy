<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Quantity_Less_Than_AllocatedOrShipped</fullName>
    <active>true</active>
    <errorConditionFormula>(Quantity_Ordered__c &lt;  Allocated_Quantity__c) || (Quantity_Ordered__c &lt;  Shipped_Quantity__c)</errorConditionFormula>
    <errorDisplayField>Quantity_Ordered__c</errorDisplayField>
    <errorMessage>Quantity can&apos;t be less than quantity allocated or quantity shipped.</errorMessage>
</ValidationRule>
